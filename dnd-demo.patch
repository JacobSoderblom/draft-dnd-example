index 0000000..c43ead9
--- /dev/null
+++ b/examples/draft-0-9-1/dnd/dnd.js
@@ -0,0 +1,375 @@
+const {Editor,EditorState,convertFromRaw} = Draft;
+const {DragLayer,DropTarget,DragDropContext,DragSource}  = ReactDnD;
+const {OrderedMap} = Immutable;
+const TouchBackend = reactDndTouchBackend;
+
+class DndSource_ extends React.Component{
+    constructor(props){
+        super(props);
+    }
+    render(){
+        let style = {...styles.container};
+        if(this.props.isDragging){
+            style.opactily=0.01;
+            style.height='50px';
+            style.overflow='hidden';
+        }
+        return <div style={style}>
+            <div style={{display:this.props.isDragging?'none':'initial'}}>
+                <img style={styles.img} src={this.props.block.getData().get('src')}/>
+                {this.props.connectDragSource(<img style={styles.dragHandle} src="tool.png"/>)}
+            </div>
+        </div>;
+    }
+}
+
+class DndPreview_ extends React.Component{
+    constructor(props){
+        super(props);
+    }
+    render(){
+        if(this.props.isDragging && this.props.currentOffset && this.props.currentOffset.y) {
+            return <div style={{...styles.preview, top: this.props.currentOffset.y}}>
+                <img style={styles.img} src={this.props.item.src}/>
+            </div>;
+        }
+        return null;
+    }
+}
+
+class DndBlock_ extends React.Component{
+    constructor(props){
+        super(props);
+    }
+    render(){
+        let content = <div data-offset-key={this.props.offsetKey}>{this.props.children}</div>
+        return this.props.connectDropTarget(content);
+    }
+}
+
+const previewCollet=(monitor)=>{
+    return {
+        isDragging: monitor.isDragging(),
+        currentOffset: monitor.getSourceClientOffset(),//monitor.getClientOffset(),
+        item:monitor.getItem(),
+        //pointOffsetY:pt.y - (src.y + DRAG_HEIGHT / 2),
+        //connectDragPreview: connect.dragPreview(),
+    };
+};
+
+const dropTarget = {
+    drop :(props, monitor, component) =>{
+        console.log(monitor.getItem().key, 'dropped on',props.blockKey);
+    },
+    hover:(props,monitor,component)=>{
+        const targetNode = ReactDOM.findDOMNode(component);
+        const targetRect = targetNode.getBoundingClientRect();
+        let targetTop = targetRect.top;
+        props.onSnap(props.blockIdx, monitor.getClientOffset().y, targetTop);
+    }
+};
+
+const targetCollect=(connect, monitor)=> {
+    return {
+        connectDropTarget: connect.dropTarget(),
+        isOver: monitor.isOver(),
+        isOverCurrent: monitor.isOver({ shallow: true }),
+        canDrop: monitor.canDrop(),
+        draggingBlock:monitor.getItem() ? monitor.getItem().key : '',
+    };
+};
+
+const sourceCollet=(connect, monitor) =>{
+    return {
+        connectDragSource: connect.dragSource(),
+        connectDragPreview: connect.dragPreview(),
+        isDragging: monitor.isDragging(),
+    };
+};
+
+const dragSource = {
+    beginDrag:(props,monitor,comp)=>{
+        props.blockProps.editor.onDrag(props.block.getKey());
+        return {
+            key:props.block.getKey(),
+            src:props.block.getData().get('src'),
+        };
+    },
+    endDrag:(props,_,comp)=>{
+        props.blockProps.editor.onDrag('');
+    }
+};
+
+const DndBlock = DropTarget('image', dropTarget, targetCollect)(DndBlock_);
+const DndSource = DragSource('image',dragSource, sourceCollet)(DndSource_);
+const DndPreview = DragLayer(previewCollet)(DndPreview_);
+
+const getInitEditorState=()=>{
+    const rawContent = {blocks:[
+        {type:'unstyled',text:'block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1 block1'},
+        {type:'unstyled',text:'block2 block2 block2 block2 block2 block2'},
+        {type:'unstyled',text:'block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3 block3'},
+        {type:'atomic',text:' ',data:{src:'img.png'}},
+        {type:'unstyled',text:'block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5 block5'},
+        {type:'atomic',text:' ',data:{src:'img2.jpg'}},
+        {type:'unstyled',text:'block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7 block7'},
+        {type:'unstyled',text:'block8 block8 block8 block8 block8 block8'},
+        {type:'unstyled',text:'block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9 block9'},
+        {type:'atomic',text:' ',data:{src:'img.png'}},
+    ]
+    ,entityMap:{}};
+    return EditorState.createWithContent(convertFromRaw(rawContent));
+};
+
+class MyEditor extends React.Component{
+    constructor(props){
+        super(props);
+        this.onChange = (editorState) => this.setState({editorState});
+        this.blockWrapper=this._blockWrapper.bind(this);
+        this.blockRenderer=this._blockRenderer.bind(this);
+        this.onSnap=this._onSnap.bind(this);
+        this.state={
+            editorState:getInitEditorState(),
+            spaceIdx:-1,
+            dragging:'',
+        };
+        this.dragging={
+            blockHeights:{},
+            blockKeys:[],
+            spaceTop:0,
+            lastDraggingMid:-10000,
+            autoScrollDrag:this._autoScrollDraggingIntervalCB.bind(this),
+            autoScrollDragInterval:-1,
+        };
+    }
+    _autoScrollDraggingIntervalCB(){
+        const spaceIdx = this.state.spaceIdx;
+        const {lastDraggingMid,blockKeys}=this.dragging;
+
+        if(spaceIdx>=0 && lastDraggingMid>0) {
+            const draggingBottom = lastDraggingMid + 50 / 2;
+            const draggingTop = lastDraggingMid - 50 / 2;
+            if (spaceIdx != blockKeys.length - 1 && draggingBottom > window.innerHeight) {
+                document.body.scrollTop += 5;
+            }
+
+            if (spaceIdx != 0 && draggingTop < 0) {
+                document.body.scrollTop -= 5;
+            }
+        }
+    }
+    _blockRenderer(block){
+        if(block.getType()==='atomic'){
+            return{
+                component: DndSource,
+                editable:false,
+                props: {
+                    editor: this,
+                }
+            }
+        }
+    };
+    onDrag(key){
+        key && console.log('onDrag',key);
+        key || console.log('onDrop',this.state.spaceIdx);
+
+        const newState = {
+            dragging:key,
+            spaceIdx :-1,
+        };
+        this.dragging.blockHeights={};
+        this.dragging.blockKeys=[];
+        this.dragging.spaceTop=-1;
+        this.dragging.lastDraggingMid=-10000;
+        clearInterval(this.dragging.autoScrollDragInterval);
+        this.dragging.autoScrollDragInterval=-1;
+
+        if(!key){
+            const curDragging = this.state.dragging;
+            const contentState = this.state.editorState.getCurrentContent();
+            const draggingBlock = contentState.getBlockForKey(curDragging);
+            const blockMap = contentState.getBlockMap().delete(draggingBlock.getKey());
+            const blocks = blockMap.toArray();
+            let beforeBlocks;
+            let afterBlocks;
+            if(this.state.spaceIdx >= blocks.length){
+                beforeBlocks = blocks.slice(0, blocks.length);
+                afterBlocks = [];
+            } else {
+                beforeBlocks = blocks.slice(0, this.state.spaceIdx);
+                //because the dragging block has been removed form blockMap, not need this.state.dragSpaceIdx - 1;
+                afterBlocks = blocks.slice(this.state.spaceIdx);
+            }
+            const newArray = [...beforeBlocks, draggingBlock, ...afterBlocks];
+            const newBlocks = OrderedMap(newArray.map(function (block) {
+                return [block.getKey(), block];
+            }));
+            const newContentState = contentState.merge({
+                blockMap:newBlocks,
+            });
+            let newEditorState = EditorState.push(this.state.editorState, newContentState, "drag-image");
+            this.setState(newState);
+            this.onChange(newEditorState);
+            return;
+        }
+        //we need all block height for dragging process
+        if(key){
+            this.dragging.autoScrollDragInterval = setInterval(this.dragging.autoScrollDrag,200);
+            const contentState = this.state.editorState.getCurrentContent();
+            const rootElem = document.getElementsByClassName('public-DraftEditor-content')[0].children[0];
+            const blocks = contentState.getBlockMap().toArray();
+
+            let spaceTop = 0;
+            let spaceTopGot = false;
+            for(let i=0;i<blocks.length;i++){
+                const isDraggingBlock = blocks[i].getKey() === key;
+                const blockKey = blocks[i].getKey();
+                this.dragging.blockHeights[blocks[i].getKey()] = isDraggingBlock?50: rootElem.children[i].offsetHeight;
+                this.dragging.blockKeys.push(blockKey);
+                if(!spaceTopGot){
+                    if(isDraggingBlock)
+                        spaceTopGot = true;
+                    else
+                        spaceTop += this.dragging.blockHeights[blockKey];
+                }
+            }
+            this.dragging.spaceTop = spaceTop;
+            newState.spaceIdx = this.dragging.blockKeys.indexOf(key);
+            console.log('begin drag:',key, this.dragging.blockKeys);
+        }
+        this.setState(newState);
+    }
+    _onSnap(overIdx, draggingMid, overTop){
+        const {blockKeys,blockHeights}=this.dragging;
+        const overKey = blockKeys[overIdx];
+        const spaceIdx = blockKeys.indexOf(this.state.dragging);
+
+        let overTopFrom0 = 0;//top of whom we are dragging over
+        blockKeys.every(b=>{
+            if(b==overKey)
+                return false;
+            overTopFrom0 += blockHeights[b];
+            return true;
+        });
+        const draggingTop = draggingMid - 50 / 2;
+        const draggingBottom = draggingTop + 50;
+        const toReal = overTop - overTopFrom0;//delta of screen.y and draft-blocks-container.y
+        const initSpaceTop = this.dragging.spaceTop + toReal;
+
+        let topSpace = draggingTop - initSpaceTop;
+        let bottomSpace = initSpaceTop + 50 - draggingBottom;
+        let moveCount = 0;
+        this.dragging.lastDraggingMid = draggingMid;
+
+        //console.log('onSnap:',topSpace,bottomSpace);
+        if(topSpace > 0) {
+            //move space down until no more TopSpace for block
+            for (let i = spaceIdx + 1; i < blockKeys.length; i++) {
+                const blockHeight = blockHeights[blockKeys[i]];
+                topSpace -= blockHeight;
+                if (topSpace < 3) {//overlap
+                    break;
+                }
+                moveCount++;
+                this.dragging.spaceTop+=blockHeight;
+            }
+            if (moveCount != 0) {
+                const moved = blockKeys.splice(spaceIdx, moveCount + 1);//remove the space itself
+                moved.splice(0,1);//remove space
+                blockKeys.splice(spaceIdx, 0, ...moved, this.state.dragging);
+                console.log('move up',moveCount);
+                this.setState({spaceIdx: blockKeys.indexOf(this.state.dragging)});
+            }
+            return;
+        }else {
+            if (bottomSpace > 0) {
+                for (let i = spaceIdx - 1; i >= 0; i--) {
+                    const blockHeight = blockHeights[blockKeys[i]];
+                    bottomSpace -= blockHeight;
+                    if (bottomSpace < 3) {
+                        break;
+                    }
+                    moveCount++;
+                    this.dragging.spaceTop -= blockHeight;
+                }
+                if (moveCount != 0) {
+                    blockKeys.splice(spaceIdx, 1);//remove the space itself
+                    blockKeys.splice(spaceIdx - moveCount, 0, this.state.dragging);
+                    console.log('move down',moveCount);
+                    this.setState({spaceIdx: blockKeys.indexOf(this.state.dragging)});
+                }
+                return;
+            }
+        }
+    }
+    _blockWrapper(blocks, offsetkeys, elements) {
+        const result = [];
+        let draggingIdxInBlock = -1;
+        elements.forEach((e, i)=> {
+            if (blocks[i].getKey() === this.state.dragging) {
+                //remove the dragging block from elements
+                draggingIdxInBlock=i;
+                return;
+            }
+            result.push(<DndBlock
+                blockIdx={i}
+                blockKey={blocks[i].getKey()}
+                onSnap={this.onSnap}
+                offsetKey={offsetkeys[i]}
+                key={blocks[i].getKey()}>{e}</DndBlock>);
+        });
+
+        if(this.state.dragging){
+            //insert the dragging block to right position
+            result.splice(this.state.spaceIdx, 0, <DndBlock
+                blockIdx={this.state.spaceIdx}
+                blockKey={this.state.dragging}
+                onSnap={this.onSnap}
+                offsetKey={offsetkeys[draggingIdxInBlock]}
+                key={this.state.dragging}>{elements[draggingIdxInBlock]}</DndBlock>);
+        }
+        return result;
+    }
+    render(){
+        return <div style={{position:'relative'}}>
+            <Editor
+                editorState={this.state.editorState}
+                onChange={this.onChange}
+                snapAt={this.state.spaceIdx.toString()}
+                blockRendererFn={this.blockRenderer}
+                blockWrapperFn={this.blockWrapper}
+                ref="editor"
+            />
+            <DndPreview editor={this}/>
+        </div>
+    }
+}
+
+const styles={
+    preview:{
+        position:'fixed',
+        width:'100%',
+        zIndex:2,
+        height:'50px',
+        overflowY:'hidden',
+        pointerEvents: 'none',
+    },
+    container:{
+        position:'relative',
+        width:'100%',
+    },
+    img:{
+        width:'100%',
+    },
+    dragHandle:{
+        position:'absolute',
+        top:'10px',
+        right:'10px',
+    },
+};
+
+const DndEditor = DragDropContext(TouchBackend)(MyEditor);
+ReactDOM.render(
+    <DndEditor />,
+    document.getElementById('target')
+);
